! Xcursor.size: 16W
URxvt.urgentOnBell: true
URxvt.scrollBar: false
!URxvt.font: xft:Anonymice Powerline:size=14:antialias=true
URxvt.font: xft:Inconsolata\ for\ Powerline:size=16:antialias=true
! URxvt.letterSpace: -1
! URxvt.depth: 24
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select
URxvt*keysym.C-F11: perl:fullscreen:switch
URxvt*borderless: true
URxvt*background.border: false
! number of lines in buffer
URxvt*saveLines:		10000